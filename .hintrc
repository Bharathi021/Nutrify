{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "meta-viewport": "off",
    "meta-charset-utf-8": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent"
        ]
      }
    ],
    "css-prefix-order": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 131",
    "not and_ff <= 132",
    "not chrome <= 131",
    "not edge <= 131",
    "not firefox <= 133",
    "not ios_saf <= 18.2",
    "not opera <= 114",
    "not safari <= 18.2",
    "not samsung <= 27"
  ]
}